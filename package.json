{
  "name": "array-of-objects-utilities",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Utilities to reduce depths of objects and convert to text",
  "keywords": [],
  "author": "Luc Patiny",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf coverage lib",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "test": "npm run test-only && npm run check-types && npm run eslint && npm run prettier",
    "test-only": "vitest run --coverage",
    "tsc": "npm run clean && npm run tsc-build",
    "tsc-build": "tsc --project tsconfig.build.json"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@zakodium/tsconfig": "^1.0.2",
    "eslint": "^9.39.2",
    "eslint-config-cheminfo-typescript": "^21.0.1",
    "openchemlib": "^9.18.2",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/array-of-objects-utilities.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/array-of-objects-utilities/issues"
  },
  "homepage": "https://github.com/cheminfo/array-of-objects-utilities#readme",
